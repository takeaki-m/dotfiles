{
  "title": "hotkeys to switch app",
  "rules":
  [
    {
      "description": "ctrl-3 to Google Calendar",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"$HOME/Applications/Chrome Apps.localized/Calendar.app\""
            }
          ]
        }
      ]
    },
    {
      "description": "ctrl-2 to chrome",
      "manipulators": [
        {
          "type": "basic",
