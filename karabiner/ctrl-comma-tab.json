{
  "title": "convert ctrl-comma to tab",
  "rules": [
    {
      "description": "convert ctrl-comma to tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab"
            }
          ]
        }
      ]
    }
  ]
}
