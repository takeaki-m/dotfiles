{
    "title": "Change ctrl+i to TAB",
    "rules": [{
        "description": "Change ctrl+i to TAB",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "i",
                "modifiers": {
                    "mandatory": ["control"]
                }
            },
            "to": [{
                "key_code": "tab"
            }]
        }]
    }]
}

