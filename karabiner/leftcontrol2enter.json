{
  "title": "space-berをenterに変更する",
  "rules": [
    {
      "description": "space-berをenterに変更する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
                    "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome$",
                "^com\\.tinyspeck\\.slackmacgap$",
                "^com\\.apple\\.Safari$",
                "^notion\\.id\\$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
