{
  "title": "space-berをenterに変更する",
  "rules": [
    {
      "description": "space-barをenterに変更する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^at\\.eggerapps\\.Postico$",
                "^net\\.java\\.openjdk\\.java$",
                "^com\\.jetbrains\\.WebStorm$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Jをenterに変更する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
              ]
            }
          ]
        }
      ]
    }
  ]
}
