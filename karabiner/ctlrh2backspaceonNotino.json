{
  "title": "Notionにてctrlhをbackspaceに変換する",
  "rules": [
    {
      "description": "Notionにてctrlhをbackspaceに変換する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "backspace"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\\\.pushplaylabs\\\\.sidekick$"
              ]
            }
          ]
        }
      ]
    }
  ]
}
